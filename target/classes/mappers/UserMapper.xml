<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="swtech.pageDesignControl.mapper.UserMapper">
    <resultMap id="BaseResultMap" type="swtech.pageDesignControl.entity.User">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="phone" property="phone" jdbcType="VARCHAR"/>
        <result column="password" property="password" jdbcType="VARCHAR"/>
        <result column="user_role" property="userRole" jdbcType="INTEGER"/>
        <result column="user_name" property="userName" jdbcType="VARCHAR"/>
        <result column="user_card" property="userCard" jdbcType="VARCHAR"/>
    </resultMap>
    <sql id="Base_Column_List">
        id,
        phone,
        password,
        user_role,
        user_name,
        user_card
    </sql>
    
    <select id="selectJobWanted" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        SELECT u.* FROM USER u WHERE  u.id=#{id}
    </select>


</mapper>
