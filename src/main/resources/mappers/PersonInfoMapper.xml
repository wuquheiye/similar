<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="job.mapper.PersonInfoMapper">
    <select id="selectByPersonUserId" resultType="job.entity.PersonInfo" parameterType="java.lang.Integer">
        SELECT * from person_info where person_user_id = #{personUserId}
    </select>
</mapper>
