<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="swtech.pageDesignControl.mapper.AttendanceMapper">
    <select id="selectByPageAndCondition" resultType="swtech.pageDesignControl.entity.Attendance"
            parameterType="swtech.pageDesignControl.entity.Attendance">
        SELECT * FROM attendance
        LIMIT #{pageStart},#{pageSize}
    </select>

    <select id="selectCount" resultType="java.lang.Integer">
        SELECT count(*) FROM attendance
    </select>
</mapper>
